// or
int binary(int *a, int n, int x, int l,int h )
{
	int mid=(l+h)/2;
	if(l>h)
	{
		return -1;
	}
	else if(a[mid]==x)
	{
		return mid;
	}
	else if(a[mid]<x)
	{
		return binary(a,n,x,mid+1,h);
	}
	else 
	{
		binary(a,n,x,l,mid+1);
	}
}

int main()
{
		int n,i=0;
	scanf("%d",&n);
	int a[n];
	for(int i=0; i<n; i++)
	{
	  scanf("%d",&a[i]);
	}
	int x;
	scanf("%d",&x);
	int l=0;
	int h=n-1;
	int res=binary(a,n,x,l,h);
	printf("%d",res);
}
